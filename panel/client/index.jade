!!! 5
html(ng-app='jungles')
  head
    title=base_url
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href=base_url + '/stylus/general.css')
    link(rel='stylesheet', href=base_url + '/custom/css')
    link(rel='icon', type='image/ico', href=base_url + '/favicon.ico')
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    base(href=base_url + '/')

  body(ng-controller='PageCtrl')

    ng-include(src='"partials/header.html"')
    ng-include(src='"partials/alerts.html"')

    input#ResourceUrl(type='hidden', value=resource_url)

    div.page(ng-view)

    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js')
    script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.1.4/angular.min.js')
    script(src='javascript/panel.js')
    script(src='custom/javascript')
